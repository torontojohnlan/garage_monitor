{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es2017",
    "sourceMap": true,
    "inlineSources": true,
    "rootDir": "./src",
    "outDir": ".",
    "lib": [
      "es2017",
      "dom"
    ],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictNullChecks": true,
  },
  "include": [
    "."
  ],
  "exclude": [
    "./.vscode"
  ]
}

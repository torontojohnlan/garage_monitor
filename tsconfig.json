{
  "compilerOptions": {
    "module": "ESNext",
    "target": "es2017",
    "sourceMap": true,
    "inlineSources": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist"
  }
}
